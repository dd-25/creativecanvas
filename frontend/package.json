{
  "name": "canvas-builder-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && npm run build:safe",
    "prebuild": "node prebuild-cleanup.js",
    "build:safe": "npm run build:vite-safe || npm run build:vite || npm run build:webpack || npm run build:esbuild || npm run build:complete-fallback",
    "build:vite": "vite build",
    "build:vite-safe": "vite build --config vite.config.safe.js",
    "build:webpack": "webpack --config webpack.config.js",
    "build:fallback": "npm run build:vite || npm run build:esbuild",
    "build:clean": "rm -rf node_modules package-lock.json && npm install && npm run build",
    "build:esbuild": "node build-esbuild.js",
    "build:complete-fallback": "node build-complete-fallback.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "esbuild": "^0.19.12",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "rollup": "^3.29.4",
    "terser": "^5.26.0",
    "vite": "^4.5.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "rollup": "^3.29.4",
    "@rollup/rollup-linux-x64-gnu": "npm:@rollup/rollup-linux-x64-musl@^3.29.4"
  }
}
